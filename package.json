{
  "name": "antlr-pglang-demo",
  "version": "0.1.0",
  "description": "The demo for the made-up programming language interpreter using ANTLR",
  "main": "./dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "webpack-dev-server --hot --inline --watch",
    "build": "NODE_ENV=production webpack",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "antlr:generate": "./node_modules/.bin/antlr4-tool -l ts --no-listener -o src/antlr/generated src/antlr/Playground.g4"
  },
  "keywords": [
    "antlr",
    "typescript",
    "webpack",
    "visitor"
  ],
  "author": "Sergey Eliseev",
  "license": "MIT",
  "dependencies": {
    "@types/antlr4": "^4.7.2",
    "@types/node": "^14.6.2",
    "antlr4": "^4.8.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "antlr4-tool": "^1.1.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.11.0",
    "prettier": "^2.1.1",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "ts-loader": "^8.0.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
