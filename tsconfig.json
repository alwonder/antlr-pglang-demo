{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "baseUrl": "./src",
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "downlevelIteration": true,
    "module": "commonjs",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
